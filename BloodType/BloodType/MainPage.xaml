    <Grid x:Name="LayoutRoot" Background="Transparent">

        <phone:Panorama Title="{Binding Path=LocalizedResources.ApplicationTitle, Source={StaticResource LocalizedStrings}}" Background="#FFB00000">

            <phone:PanoramaItem Header="{Binding Path=LocalizedResources.Main, Source={StaticResource LocalizedStrings}}">
                <phone:LongListSelector Margin="0,0,-22,0" ItemsSource="{Binding MainItems}" Name="MainMenu" SelectionChanged="MainMenu_SelectionChanged">
                    <phone:LongListSelector.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Margin="0,-6,0,12">
                                <TextBlock Text="{Binding LineOne}" TextWrapping="Wrap" Style="{StaticResource PhoneTextExtraLargeStyle}" FontSize="{StaticResource PhoneFontSizeExtraLarge}"/>
                            </StackPanel>
                        </DataTemplate>
                    </phone:LongListSelector.ItemTemplate>
                </phone:LongListSelector>
            </phone:PanoramaItem>

            <phone:PanoramaItem Header="{Binding Path=LocalizedResources.Settings, Source={StaticResource LocalizedStrings}}">
                <phone:LongListSelector Margin="0,0,-22,0" ItemsSource="{Binding SettingsItems}" Name="SettingsMenu" SelectionChanged="SettingsMenu_SelectionChanged">
                    <phone:LongListSelector.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Margin="0,-6,0,12">
                                <TextBlock Text="{Binding LineOne}" TextWrapping="Wrap" Style="{StaticResource PhoneTextExtraLargeStyle}" FontSize="{StaticResource PhoneFontSizeExtraLarge}"/>
                            </StackPanel>
                        </DataTemplate>
                    </phone:LongListSelector.ItemTemplate>
                </phone:LongListSelector>
            </phone:PanoramaItem>

            <phone:PanoramaItem Header="{Binding Path=LocalizedResources.About, Source={StaticResource LocalizedStrings}}">
                <Grid Margin="16,0,16,80">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="30*"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="50"/>
                        <RowDefinition Height="30"/>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Text="{Binding Path=LocalizedResources.Disclaimer, Source={StaticResource LocalizedStrings}}" HorizontalAlignment="Left" TextWrapping="Wrap"/>
                    <TextBlock Grid.Row="1" Text="{Binding Path=LocalizedResources.Notice, Source={StaticResource LocalizedStrings}}" HorizontalAlignment="Left" TextWrapping="Wrap" Margin="0,20,0,0" FontSize="20" FontStyle="Italic"/>
                    <HyperlinkButton Grid.Row="2" Content="{Binding Path=LocalizedResources.Rate, Source={StaticResource LocalizedStrings}}" HorizontalAlignment="Left" Margin="-10,0,0,0" Click="RateMe_Click"  />
                    <HyperlinkButton Grid.Row="3" Content="{Binding Path=LocalizedResources.Feedback, Source={StaticResource LocalizedStrings}}" HorizontalAlignment="Left" Margin="-10,-20,0,0" Click="EmailMe_Click"  />
                    <TextBlock Grid.Row="4" Text="{Binding Path=LocalizedResources.Release, Source={StaticResource LocalizedStrings}}" HorizontalAlignment="Left" />
                </Grid>

            </phone:PanoramaItem>

        </phone:Panorama>
